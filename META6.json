{
    "name": "Elucid8::Build",
    "description": "Renders RakuDoc sources in multiple languages to web site",
    "version": "0.10.10",
    "raku": "6.d",
    "authors": [
        "Richard Hainsworth"
    ],
    "auth": "zef:finanalyst",
    "depends": [
        "Rakuast::RakuDoc::Render",
        "RakuConfig",
        "PrettyDump",
        "File::Directory::Tree",
        "Cro::HTTP::Router",
        "Cro::HTTP::Server",
        "Cro::HTTP::Log::File",
        "Git::Blame::File"
    ],
    "test-depends": [
        "Test",
        "Test::META"
    ],
    "provides": {
        "Elucid8-Build": "lib/Elucid8-Build.rakumod",
        "Elucid8::Plugin::HTML::AutoIndex": "lib/plugins/html/AutoIndex.rakumod",
        "Elucid8::Plugin::HTML::UISwitcher": "lib/plugins/html/UISwitcher.rakumod",
        "Elucid8::Plugin::HTML::SiteMap": "lib/plugins/html/SiteMap.rakumod",
        "Elucid8-Add-ui-lang": "lib/tools/add-ui-lang.rakumod",
        "RunCroApp": "lib/tools/RunCroApp.rakumod",
        "Elucid8-Gather": "lib/Elucid8-Gather.rakumod"
    },
    "resources": [
        "config/01-base.raku",
        "config/02-plugins.raku",
        "config/03-plugin-options.raku",
        "config/04-repositories.raku",
        "ui-dictionary.rakuon"
    ],
    "support": {
        "email": "richard@hainsworth.wales",
        "bugtracker": "https://github.com/elucid8-org/build/issues",
        "source": "https://github.com/elucid8-org/build.git"
    },
    "license": "Artistic-2.0",
    "source-url": "git://github.com/elucid8-org/build.git",
    "source-type": "git"
}
